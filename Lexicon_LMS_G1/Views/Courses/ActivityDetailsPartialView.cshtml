@model Lexicon_LMS_G1.Entities.Entities.Activity

<h3 class="px-3 text-center">
    @Html.DisplayFor(a => Model.Name)
</h3>
<p class="px-3">
    <strong>Duration:</strong> @Model.StartDate.ToString("yyyy/dd/mm") <strong>-</strong> @Model.EndDate.ToString("yyyy/dd/mm")
</p>
<h5 class="px-3">Description:</h5>
<p class="px-4">
    @Html.DisplayFor(a => Model.Description)
</p>
