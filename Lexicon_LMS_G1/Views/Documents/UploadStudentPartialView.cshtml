@model DocumentUploadViewModel<string, Lexicon_LMS_G1.Entities.Entities.ApplicationUser>

<h1>Student</h1>
<form asp-action="UploadStudentDocument" enctype="multipart/form-data" asp-controller="Documents">
    <div>
        <label asp-for="Document" class="control-label"></label>
        <input asp-for="Document" name="document" type="file" class="form-control" required />
    </div>
    <div>
        <label asp-for="Description" class="control-label"></label>
        <input asp-for="Description" name="description" type="text" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Upload File</button>
</form>